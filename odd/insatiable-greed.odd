                     <attDef ident="xml:lang" mode="delete"/>
                     <attDef ident="xml:space" mode="delete"/>
                  </attList>
               </elementSpec>
                    
                    <elementSpec ident="anchor" module="linking" mode="change">
                        <desc versionDate="2016-10-21" xml:lang="en">used to indicate the beginning and end of excerpts within transcribed texts.</desc>
                        <constraintSpec ident="anchor-requires-type" scheme="schematron">
                            <constraint>
                                <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:anchor[exists(@next|@prev) or matches(@xml:id,'excerpt[0-9][0-9][0-9](Start|End)')]">
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="@type">ERROR: <name/> needs @type.</assert>
                                </rule>
                            </constraint>
                        </constraintSpec>
                        
                        <constraintSpec ident="excerptMarker-requires-connector" scheme="schematron">
                            <constraint>
                                <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:anchor[@type='excerptMarker']">
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="(@next|@prev)">ERROR: <name/>  needs either @next (if it's the start of an excerpt) or @prev (if it's the end).</assert>
                                </rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="anchor-extract-id-checks" scheme="schematron">
                            <constraint>
                                <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:anchor[@type='excerptMarker']">
                                    <let name="xmlid" value="@xml:id" />
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="(@next|@prev)">ERROR: <name/> needs @next (if marking the start of an extract) or @prev (if marking the end).</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="matches(@xml:id,'^excerpt[0-9][0-9][0-9](Start|End)$')">ERROR: <name/>/@xml:id name incorrect. It must match #excerpt{3 digits}Start (if marking the start of an excerpt) or #excerpt{3 digits}End (if marking the end).</assert>
                                </rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="anchor-next-checks" scheme="schematron">
                            <constraint>
                                <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:anchor[@next]">
                                    <let name="text" value="ancestor::tei:text"/>
                                    <let name="next" value="@next" />
                                    <let name="xmlid" value="@xml:id" />
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="matches(@next,'^#excerpt[0-9][0-9][0-9]End$')">ERROR: <name/>/@next must match the format #excerpt{digits}End (e.g. pointing to xml:id="excerpt001End").</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="not(@prev)">ERROR: <name/> with @next must not also contain @prev.</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="$text//tei:anchor[concat('#', @xml:id)=$next]">ERROR: <name/> points to nonexistent <name/>/@xml:id='<value-of select="replace($next,'^#','')"/>'.</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="concat('#',@xml:id) = $text//tei:anchor[concat('#', @xml:id)=$next]/@prev">ERROR: <name/> is not the target of the ending anchor's @prev.</assert>
                                </rule>
                            </constraint>
                        </constraintSpec>
                        <constraintSpec ident="anchor-prev-checks" scheme="schematron">
                            <constraint>
                                <rule xmlns="http://purl.oclc.org/dsdl/schematron" context="tei:anchor[@prev]">
                                    <let name="text" value="ancestor::tei:text"/>
                                    <let name="prev" value="@prev" />
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="matches(@prev,'^#excerpt[0-9][0-9][0-9]Start$')">ERROR: <name/>/@prev must match the format #excerpt{digits}Start (e.g. pointing to xml:id="excerpt001Start").</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="not(@next)">ERROR: <name/> with @prev must not also contain @next.</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="$text//tei:anchor[concat('#', @xml:id)=$prev]">ERROR: Not <name/>/@xml:id='<value-of select="replace($prev,'^#','')"/>'.</assert>
                                    <assert xmlns="http://purl.oclc.org/dsdl/schematron" test="concat('#',@xml:id) = $text//tei:anchor[concat('#', @xml:id)=$prev]/@next">ERROR: <name/> is not the target of the starting anchor's @next.</assert>
                                </rule>
                            </constraint>
                        </constraintSpec>
                        <attList>
                            <attDef ident="type" usage="req" mode="change">
                                <desc versionDate="2014-04-20" xml:lang="en">values are "<val>excerptMark</val>" (for the anchor elements marking the start/finish of the excerpt) and "<val>continueMarker</val>" (for the marker identifying where the initial display is the end with the following text hidden behind a 'More...' button).</desc>
                                <valList type="closed" mode="replace">
                                    <valItem ident="excerptMarker"/>
                                </valList>
                            </attDef>
                            <attDef ident="xml:id" usage="req" mode="change">
                                <datatype>
                                    <rng:data xmlns:rng="http://relaxng.org/ns/structure/1.0" type="ID">
                                        <rng:param name="pattern">excerpt\d{3,3}(Start|End)</rng:param>
                                    </rng:data>
                                </datatype>
                            </attDef>
                            <attDef ident="next" usage="opt" mode="change">
                                <datatype>
                                    <rng:data xmlns:rng="http://relaxng.org/ns/structure/1.0" type="string">
                                        <rng:param name="pattern">#excerpt\d{3,3}End</rng:param>
                                    </rng:data>
                                </datatype>
                            </attDef>
                            <attDef ident="prev" usage="opt" mode="change">
                                <datatype>
                                    <rng:data xmlns:rng="http://relaxng.org/ns/structure/1.0" type="string">
                                        <rng:param name="pattern">#excerpt\d{3,3}Start</rng:param>
                                    </rng:data>
                                </datatype>
                            </attDef>
                            
                            <!-- Deleting attributes -->
                            <attDef ident="ana" mode="delete" />
                            <attDef ident="change" mode="delete" />
                            <attDef ident="copyOf" mode="delete" />
                            <attDef ident="corresp" mode="delete" />
                            <attDef ident="exclude" mode="delete" />
                            <attDef ident="facs" mode="delete" />
                            <attDef ident="n" mode="delete" />
                            <attDef ident="rend" mode="delete" />
                            <attDef ident="rendition" mode="delete" />
                            <attDef ident="sameAs" mode="delete" />
                            <attDef ident="select" mode="delete" />
                            <attDef ident="style" mode="delete" />
                            <attDef ident="subtype" mode="delete" />
                            <attDef ident="synch" mode="delete" />
                            <attDef ident="xml:base" mode="delete" />
                            <attDef ident="xml:lang" mode="delete" />
                            <attDef ident="xml:space" mode="delete" />
                        </attList>
                        <exemplum xml:lang="en">
                            <egXML xmlns="http://www.tei-c.org/ns/Examples" />
